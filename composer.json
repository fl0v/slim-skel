{
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "fig/http-message-util": "^1.1",
    "monolog/monolog": "^3",
    "nyholm/psr7": "^1",
    "nyholm/psr7-server": "^1",
    "php-di/php-di": "^7",
    "slim/php-view": "^3.2",
    "slim/psr7": "^1.6",
    "slim/slim": "^4",
    "symfony/console": "^7.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3",
    "phpstan/phpstan": "1.*",
    "phpunit/phpunit": "^11.0",
    "selective/test-traits": "^4",
    "squizlabs/php_codesniffer": "^3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "tests/"
    }
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  }
}
